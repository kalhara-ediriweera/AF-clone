{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles":[
                    "dist/**"
                ]
            }
        }
    ],
    "toutes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}